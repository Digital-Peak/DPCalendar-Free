    }
  },
  "require": {
    "sabre/vobject": "^4.1",
    "sabre/dav": "^3.2",
    "tecnickcom/tcpdf": "dev-min",
    "mustache/mustache": "2.11.*",
    "omnipay/stripe": "2.4.*",
    "omnipay/paypal": "2.5.*",
    "omnipay/2checkout": "2.1.*",
    "omnipay/manual": "2.2.*",
    "league/pipeline": "^0.3.0",
    "simshaun/recurr": "^4.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/Digital-Peak/TCPDF"
    }
  ],
  "autoload": {
